
<%= user %>

<div class="topicHeading">
<h2>Climate Change</h2>
<h2>Globalism</h2>
<h1>Privacy</h1>
</div>

<br> <br>



<div class="row">

  <% for (var i = 0; i < articles.length; i++) { %>
    <div class="wrapper">
      <div class="ih-item square colored effect4 featuredArticle">
        <a href="<%= articles[i].link %>" target="_blank">
          <div class="img">
            <img src="<%= articles[i].image %>" alt="img">
          </div> 
          <div class="mask1"></div>
          <div class="mask2"></div>
          <div class="info">
            <h3><%= articles[i].title %></h3>
            <p><%= articles[i].originalSource %></p>
          </div>
          </a>
        </div>
    </div>
  <% } %>

</div>
